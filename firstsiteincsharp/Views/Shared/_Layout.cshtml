                          <img src="https://ibb.co/ngtFT9V" alt="">
                    @* <span class="navbar-toggler-icon"></span> *@
                </button>
                
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
            
                    <ul class="nav justify-content-end">
                    
  <li class="nav-item">
    <a class="nav-link active text-white" asp-area="" asp-controller="Home" asp-action="Active">Active</a>
  </li>
  <li class="nav-item">
    @* <a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Home">Home</a> *@
    <div class="dropdown">
  <button class="btn btn-secondary" type="button" data-bs-toggle="dropdown" aria-expanded="false">
    Home
  </button>
  <ul class="dropdown-menu">
    <li>
    <a class="nav-link active dropdown-item" asp-area="" asp-controller="Home" asp-action="Registration">Admin</a></li>

    <li><a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Course">Course</a></li>
    
    <li><a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Librarian">Librarian</a></li>
  </ul>
</div>
  </li>
  <li class="nav-item">
    <a class="nav-link active text-white" asp-area="" asp-controller="Home" asp-action="Registration">Registration</a>  
  </li>
  <li class="nav-item"> 
    <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
  </li>
  <li class="nav-item">
    <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Login">Login</a>
  </li>
